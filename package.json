{
  "name": "@masa-finance/masa-sdk",
  "version": "3.18.1",
  "description": "Brand new Masa SDK",
  "main": "./dist/src/index.js",
  "typings": "./dist/src/index.d.ts",
  "unpkg": "./dist/browser/masa-sdk.cjs2.min.js",
  "module": "./dist/browser/masa-sdk.amd.min.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "ts-mocha test/**/*.test.ts",
    "test:watch": "ts-mocha --extension ts,js test/**/*.test.ts --watch",
    "start": "yarn build:watch",
    "build": "yarn build:tsc && yarn build:dist",
    "build:tsc": "tsc",
    "build:dist": "cross-env NODE_ENV=production webpack",
    "build:watch": "tsc -w",
    "prettier": "prettier ./src ./test ./webpack.*.ts --write",
    "lint": "eslint ./src ./test ./webpack.*.ts",
    "doc": "typedoc",
    "prepublishOnly": "yarn build",
    "publish-lib": "npm publish",
    "publish-beta": "npm publish --tag beta",
    "publish-dryrun": "npm publish --dry-run",
    "clean": "rm -rf ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/masa-finance/masa-sdk.git"
  },
  "keywords": [
    "De-fi"
  ],
  "author": "Masa Finance",
  "license": "MIT",
  "dependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/abstract-provider": "^5.7.0",
    "@ethersproject/abstract-signer": "^5.7.0",
    "@ethersproject/bignumber": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@layerzerolabs/lz-definitions": "^2.0.25",
    "@layerzerolabs/lz-v2-utilities": "^2.0.25",
    "@masa-finance/masa-contracts-identity": "^1.12.0",
    "@masa-finance/masa-token": "^2.0.8",
    "arweave": "1.11.8",
    "axios": "^1.6.7",
    "ethers": "~5.7.2",
    "grapheme-splitter": "^1.0.4",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/chai": "^4.3.11",
    "@types/circular-dependency-plugin": "^5.0.8",
    "@types/mocha": "^10.0.6",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^6.21.0",
    "chai": "~4.4.1",
    "circular-dependency-plugin": "^5.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.25.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "mocha": "^10.2.0",
    "prettier": "^3.1.0",
    "terser-webpack-plugin": "^5.3.6",
    "ts-mocha": "^10.0.0",
    "typedoc": "^0.25.7",
    "typedoc-plugin-markdown": "^3.17.0",
    "webpack": "^5.84.0",
    "webpack-cli": "^5.0.1",
    "webpack-merge": "^5.10.0"
  }
}
